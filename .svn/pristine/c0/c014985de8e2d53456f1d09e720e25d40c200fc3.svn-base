/***workbench工作台***/
.workbench_box {
}

.workbench_box .banner {
    padding: 0 20rpx;
}

.workbench_box .banner image {
    width: 100%;
    height: 280rpx;
    border-radius: 30rpx;
    margin: 0 auto;
    display: block;
    box-shadow: 0 10rpx 10rpx #ddd;
}

.workbench_box .section_box {
    display: flex;
    margin-top: 60rpx;
    flex-wrap: wrap;
}

.workbench_box .section_box .section {
    width: 25%;
    height: 150rpx;
    text-align: center;
    margin-bottom: 40rpx;
}

.workbench_box .section_box .section view {
    width: 100rpx;
    height: 100rpx;
    margin: 0 auto;
}

.workbench_box .section_box .section span {
    display: block;
    font-size: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
    margin-top: 20rpx;
}


.workbench_box .count_box .tit {
    font-size: 32rpx;
    width: 100%;
    height: 36rpx;
    line-height: 36rpx;
    padding: 20rpx 20rpx 40rpx 20rpx;
}

.workbench_box .count_box {
    display: flex;
    flex-wrap: wrap;
    border-top: 10rpx solid #eee;
}

.count_list {
    width: 25%;
    height: 190rpx;
    text-align: center;
    margin-bottom: 40rpx;
}

.count_list .loading h3 {
    display: block;
    font-size: 30rpx;
    font-weight: 600;
    padding-top: 25rpx;
}

.count_list .loading span {
    display: block;
    font-size: 24rpx;
    color: #999;
    font-weight: 400;
}

.count_list .loading {
    margin: 0 auto;
    width: 140rpx;
    height: 140rpx;
    position: relative;
    margin-bottom: 18rpx;
}

.count_list .loading .progress {
    position: absolute;
    width: 120rpx;
    height: 120rpx;
    background-color: white;
    border-radius: 50%;
    left: 10rpx;
    top: 10rpx;
    text-align: center;
}

.count_list .left,
.count_list .right {
    width: 70rpx;
    height: 140rpx;
    overflow: hidden;
    position: relative;
    float: left;
    background-color: #ccc;
}

.count_list .left {
    border-radius: 140rpx 0 0 140rpx;
}

.count_list .right {
    border-radius: 0 140rpx 140rpx 0;
}

.count_list .left:after,
.count_list .right:after {
    content: "";
    position: absolute;
    display: block;
    width: 70rpx;
    height: 140rpx;
    background-color: white;
    border-radius: 140rpx 0 0 140rpx;
    background-color: red;
}

.count_list .right:after {
    content: "";
    position: absolute;
    display: block;
    border-radius: 0 140rpx 140rpx 0;
}

.count_list .left:after {
    transform-origin: right center;
}

.count_list .right:after {
    transform-origin: left center;
    transform: rotateZ(135deg);
}

/***统计区的颜色  start***/
/*报备*/
.baobei .left:after, .baobei .right:after {
    background-color: #70c3b0;
}

.baobei h3 {
    color: #70c3b0;
}

/*带看*/
.daikan .left:after, .daikan .right:after {
    background-color: #f1c253;
}

.daikan h3 {
    color: #f1c253;
}

/*成交*/
.chengjiao .left:after, .chengjiao .right:after {
    background-color: #eb5d5d;
}

.chengjiao h3 {
    color: #eb5d5d;
}

/*结佣*/
.jieyong .left:after, .jieyong .right:after {
    background-color: #50b1e5;
}

.jieyong h3 {
    color: #50b1e5;
}

/*收藏*/
.shouchang .left:after, .shouchang .right:after {
    background-color: #eb5d5d;
}

.shouchang h3 {
    color: #eb5d5d;
}

/*转发*/
.zhuanfa .left:after, .zhuanfa .right:after {
    background-color: #50b1e5;
}

.zhuanfa h3 {
    color: #50b1e5;
}

/***统计区的颜色  end***/

/***上传图片 start**/
.section_img {
    padding: 0 20rpx;
    line-height: 90rpx;
    font-size: 28rpx;
    font-weight: 500;
    margin-bottom: 20rpx;
}

.section_img span {
    font-size: 24rpx;
    color: red;
}

.section_img .upload_image {
    display: flex;
}

.section_img .upload_image view {
    width: 50%;
    height: 162rpx;
    position: relative;
}

.section_img .upload_image image {
    width: 290rpx;
    height: 162rpx;
    background-color: #eee;
    display: block;
    margin: 0 auto;
    border-radius: 10rpx;
}

.section_img .upload_image span {
    position: absolute;
    left: 0;
    top: 30rpx;
    width: 100%;
    text-align: center;
    color: #999;
    font-size: 26rpx;
}
/***上传图片 end**/

/***后台房源上传图片**/
.house_img{padding:0 20rpx;margin-bottom: 20rpx;}
.house_img .tit{line-height: 90rpx;font-size: 28rpx;font-weight: 500;}
.house_img .tit span{ font-size: 24rpx;color: red;}
.house_img .image_box{display: flex;flex-wrap: wrap;}
.house_img .image_box .image_li{position:relative;margin-right: 10rpx;width: 200rpx;text-align:center;}
.house_img .image_box image{width: 200rpx;height:200rpx;background-color: #f2f2f2;}
.house_img .image_box .image_li .img_dec{display: block;position: absolute; top:50%;margin-top: -40rpx; width: 100%;height: 40rpx;line-height: 40rpx;color: #777;}
.house_img .image_box .image_li .img_dec span{display: block;}
.house_img .image_box .image_li .hidden{display: none}

/*checkbox 选项框大小  */
checkbox .wx-checkbox-input {
    width: 36rpx;
    height: 36rpx;
    border-radius: 50%;
}

/*checkbox选中后样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
    background: #1296db;
}

/*checkbox选中后图标样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    width: 28rpx;
    height: 28rpx;
    line-height: 28rpx;
    text-align: center;
    font-size: 22rpx;
    color: #fff;
    background: transparent;
    transform: translate(-50%, -50%) scale(1);
    -webkit-transform: translate(-50%, -50%) scale(1);
}


