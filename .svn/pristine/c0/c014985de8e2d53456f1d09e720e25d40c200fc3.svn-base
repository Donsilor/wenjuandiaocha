/****按钮样式*******/
button{padding: 0;}
.btn {
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 14px;
    padding-right: 14px;
    box-sizing: border-box;
    font-size: 18px;
    text-align: center;
    text-decoration: none;
    line-height: 2.55555556;
    border-radius: 5px;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    color: #000000;
    background-color: #F8F8F8;
}

.btn-default {
    color: #444;
    background-color: #e6e6e6;
    border-color: #e6e6e6;
}

.btn-primary {
    color: #fff;
    background-color: #fda214;
    border-color: #fda214;
}

.btn-secondary {
    color: #fff;
    background-color: #50b5fd;
    border-color: #50b5fd;
}

.btn-success {
    color: #fff;
    background-color: #5eb95e;
    border-color: #5eb95e;
}

.btn-warning {
    color: #fff;
    background-color: #F37B1D;
    border-color: #F37B1D;
}

.btn-danger {
    color: #fff;
    background-color: #dd514c;
    border-color: #dd514c;
}


